{% extends 'SonataAdminBundle::standard_layout.html.twig' %}

{% block breadcrumb %}
    <li>
        <a href="{{ url('sonata_admin_dashboard') }}"><i class="fa fa-home"></i></a>
    </li>
    <li class="active">Custom Page</li>
{% endblock %}

{% block content %}
    <h1>Import</h1>
    {{ form_start(form, {'attr': {'class': 'search__i'}}) }}
        {{ form_errors(form) }}
        <table class="table table-hover">
            <tbody>
            <tr>
                <td>
                    {{ form_widget(form.type) }}
                </td>
                <td class="sonata-ba-list-label">
                    {{ form_widget(form.file) }}
                </td>
                <td>
                    <div class="btn-group">
                        {{ form_widget(form.submit) }}
                    </div>
                </td>
            </tr>
            </tbody>
        </table>
    {{ form_end(form) }}
{% endblock %}
