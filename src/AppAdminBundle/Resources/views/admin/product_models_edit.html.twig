{% extends 'AppAdminBundle:admin:products_base_edit.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    {% stylesheets 'bundles/appadmin/sass/admin.scss' filter='compass' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {#<link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">#}
    {% endstylesheets %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {'callback': 'fos.Router.setData'}) }}"></script>
    <script src="{{ asset('bundles/appadmin/js/lib/jquery-ui.min.js') }}"></script>
    <script src="{{ asset('bundles/appadmin/js/jquery.filedrop.js') }}"></script>
    <script src="{{ asset('bundles/appadmin/js/js.fileDropBlock.js') }}"></script>
    <script src="{{ asset('bundles/appadmin/js/js.fileLoadByDefault.js') }}"></script>
    <script src="{{ asset('bundles/appadmin/js/products.js') }}"></script>
{% endblock %}
