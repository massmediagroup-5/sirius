<div class="col-sm-12" id="orderHistoryItems">
    <div class="box box-success">
        <div class="box-header">
            <h4 class="box-title">История</h4>
        </div>
        <div class="box-body order-history">
            <ul>
                {% for historyItem in admin.subject.history %}
                    <li>
                        {{ admin.getHistoryItemLabel(historyItem) }}
                        <a href="{{ path(admin.baseRouteName ~ '_cancel_order_change', {id: admin.subject.id, history_id: historyItem.id}) }}">
                            Отменить</a>
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>
</div>